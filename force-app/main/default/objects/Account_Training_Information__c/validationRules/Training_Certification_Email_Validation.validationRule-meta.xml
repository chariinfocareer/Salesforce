<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Training_Certification_Email_Validation</fullName>
    <active>false</active>
    <errorConditionFormula>AND( !ISBLANK( Training_Cerificate_Number__c ) , ISBLANK( Training_Certificate_Link__c ))</errorConditionFormula>
    <errorDisplayField>Training_Certificate_Link__c</errorDisplayField>
    <errorMessage>Fill the Training Certificate Link Field</errorMessage>
</ValidationRule>
