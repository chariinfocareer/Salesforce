<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Invoice_Email_Validation</fullName>
    <active>false</active>
    <errorConditionFormula>AND( ISBLANK( Invoice_Number__c ) ,!ISBLANK( Invoice_Number_Link__c ) )</errorConditionFormula>
    <errorDisplayField>Invoice_Number__c</errorDisplayField>
    <errorMessage>Fill the Invoice Number Field</errorMessage>
</ValidationRule>
